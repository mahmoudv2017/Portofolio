doctype html

-var pageTitle = 'HomePage'
html(lang="en")
    head
        
        include pug/layouts/meta

        include pug/layouts/styles
        title #{pageTitle}
    body 
        include pug/layouts/sidebar
        .main-div 
            include pug/layouts/host-data
            .btton
                | Menu
                img(src="css/static/icons/menu.svg") 
            .exit-btn
                img(src="css/static/icons/close.svg", alt="", srcset="")
        
    include pug/layouts/scripts